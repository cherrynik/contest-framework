    "@typescript-eslint/parser": "^4.33.0",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
