  "version": "1.0.0",
  "description": "A framework for competitive programming solutions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.22.9 <13.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
